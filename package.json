{
  "name": "azure-chatgpt-demo",
  "version": "9.2.0",
  "description": "Advanced Azure OpenAI chat application with GPT-5 series, O-series reasoning models, Sora video generation, and real-time voice chat capabilities.",
  "main": "index.js",
  "scripts": {
    "start": "node ./server.js",
    "dev": "npm-run-all --parallel dev:server dev:webpack",
    "dev:server": "nodemon ./server.js",
    "dev:webpack": "webpack --watch",
    "build": "webpack",
    "test:mocha": "mocha --timeout 5000 ./tests/profile.test.js",
    "test:jest": "Jest",
    "test:new-models": "mocha --timeout 30000 ./tests/newModels.test.js",
    "test:integration": "mocha --timeout 60000 ./tests/newModelsIntegration.test.js",
    "test:performance": "mocha --timeout 120000 ./tests/newModelsPerformance.test.js",
    "test:e2e": "mocha --timeout 90000 ./tests/newModelsE2E.test.js",
    "test:all-new": "npm run test:new-models && npm run test:integration && npm run test:performance && npm run test:e2e",
    "test": "npm run test:mocha && npm run test:jest && npm run test:all-new"
  },
  "jest": {
    "testEnvironment": "node",
    "testMatch": [
      "**/tests/**/*.?(m)js?(x)",
      "**/?(*.)+(spec|test).?(m)js?(x)"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/tests/profile.test.js"
    ],
    "transform": {
      "^.+\\.[t|j]sx?$": "babel-jest"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hddevteam/Azure-chatGPT-demo.git"
  },
  "keywords": [
    "azure",
    "openai",
    "chatgpt",
    "gpt-5",
    "o-series",
    "sora",
    "video-generation",
    "realtime-voice",
    "ai",
    "pwa"
  ],
  "author": "HDDevTeam",
  "license": "ISC",
  "dependencies": {
    "@azure/data-tables": "^13.2.2",
    "@azure/msal-browser": "^3.13.0",
    "@azure/msal-node": "^2.7.0",
    "@azure/storage-blob": "^12.17.0",
    "axios": "^1.3.5",
    "cheerio": "^1.0.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "fs-extra": "^11.1.1",
    "html-to-text": "^9.0.5",
    "katex": "^0.16.11",
    "mammoth": "^1.9.0",
    "marked-katex-extension": "^5.0.2",
    "microsoft-cognitiveservices-speech-sdk": "^1.27.0",
    "multer": "^1.4.5-lts.1",
    "passport": "^0.7.0",
    "passport-azure-ad": "^4.3.5",
    "pdf-parse": "^1.1.1",
    "remove-markdown": "^0.5.0",
    "request": "^2.88.2",
    "rt-client": "https://github.com/Azure-Samples/aoai-realtime-audio-sdk/releases/download/js/v0.5.0/rt-client-0.5.0.tgz",
    "sweetalert": "^2.1.2",
    "unzipper": "^0.12.3",
    "wss": "^3.3.4",
    "xlsx": "^0.18.5",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.23.3",
    "@types/uuid": "^9.0.6",
    "babel-jest": "^29.7.0",
    "chai": "^4.3.7",
    "chai-http": "^4.3.0",
    "clipboard": "^2.0.11",
    "dotenv-webpack": "^8.1.0",
    "eslint": "^8.38.0",
    "eslint-plugin-jest": "^27.6.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jquery": "^3.6.4",
    "marked": "^4.3.0",
    "mocha": "^10.2.0",
    "nodemon": "^3.0.1",
    "npm-run-all": "^4.1.5",
    "opus-media-recorder": "^0.8.0",
    "popper.js": "^1.16.1",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4"
  }
}
